<?xml version="1.0" encoding="utf-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
  <mime-type type="application/x-cubeworld">
    <comment>Voxel model CubeWorld</comment>
    <glob pattern="*.cub"/>
  </mime-type>
  <mime-type type="application/x-binvox">
    <comment>Voxel model binvox</comment>
    <glob pattern="*.binvox"/>
  </mime-type>
  <mime-type type="application/x-polygonfileformat">
    <comment>Polygon File Format (Stanford Triangle Format)</comment>
    <glob pattern="*.ply"/>
  </mime-type>
  <mime-type type="application/x-goxel">
    <comment>Voxel model Goxel</comment>
    <glob pattern="*.gox"/>
    <magic priority="50">
      <match type="string" offset="0" value="GOX "/>
    </magic>
  </mime-type>
  <mime-type type="application/x-magicavoxel">
    <comment>Voxel model MagicaVoxel</comment>
    <magic priority="50">
      <match type="string" offset="0" value="VOX "/>
    </magic>
    <glob pattern="*.vox"/>
  </mime-type>
  <mime-type type="application/x-magicavoxel-xraw">
    <comment>MagicaVoxel XRAW</comment>
    <magic priority="50">
      <match type="string" offset="0" value="XRAW"/>
    </magic>
    <glob pattern="*.xraw"/>
  </mime-type>
  <mime-type type="application/x-voxelmax">
    <comment>VoxelMax</comment>
    <glob pattern="*.vmax.zip"/>
    <glob pattern="*.vmaxb"/>
  </mime-type>
  <mime-type type="application/x-minetest">
    <comment>Minetest Schematic File</comment>
    <glob pattern="*.mts"/>
  </mime-type>
  <mime-type type="application/x-minecraftregion">
    <comment>Minecraft region file</comment>
    <glob pattern="*.mca"/>
    <glob pattern="*.mcr"/>
  </mime-type>
  <mime-type type="application/x-minecraftlevel">
    <comment>Minecraft level file</comment>
    <glob pattern="*.dat"/>
  </mime-type>
  <mime-type type="application/x-minecraftschematic">
    <comment>Minecraft schematic file</comment>
    <glob pattern="*.schem"/>
    <glob pattern="*.schematic"/>
    <glob pattern="*.nbt"/>
  </mime-type>
  <mime-type type="application/x-starmade">
    <comment>StarMade model</comment>
    <glob pattern="*.sment"/>
  </mime-type>
  <mime-type type="application/x-qubicle-binary">
    <comment>Voxel model Qubicle binary</comment>
    <glob pattern="*.qb"/>
  </mime-type>
  <mime-type type="application/x-qubicle-tree">
    <comment>Voxel model Qubicle tree</comment>
    <magic priority="50">
      <match type="string" offset="0" value="QB 2"/>
    </magic>
    <glob pattern="*.qbt"/>
  </mime-type>
  <mime-type type="application/x-qubicle-exchange">
    <comment>Voxel model Qubicle Exchange Format</comment>
    <magic priority="50">
      <match type="string" offset="0" value="Qubicle Exchange Format"/>
    </magic>
    <glob pattern="*.qef"/>
  </mime-type>
  <mime-type type="application/x-qubicle-project">
    <comment>Voxel model Qubicle Project</comment>
    <magic priority="50">
      <match type="string" offset="0" value="QBCL"/>
    </magic>
    <glob pattern="*.qbcl"/>
  </mime-type>
  <mime-type type="application/x-sandbox-voxedit">
    <comment>Voxel model Sandbox VoxEdit</comment>
    <glob pattern="*.vxm"/>
    <magic priority="50">
      <match type="string" offset="0" value="VXM"/>
    </magic>
  </mime-type>
  <mime-type type="application/x-sandbox-voxedit-vxr">
    <comment>Voxel model Sandbox VoxEdit</comment>
    <glob pattern="*.vxr"/>
    <magic priority="50">
      <match type="string" offset="0" value="VXR"/>
    </magic>
  </mime-type>
  <mime-type type="application/x-sandbox-voxedit-vxt">
    <comment>Voxel tileset Sandbox VoxEdit</comment>
    <glob pattern="*.vxt"/>
    <magic priority="50">
      <match type="string" offset="0" value="VXT"/>
    </magic>
  </mime-type>
  <mime-type type="application/x-sandbox-voxedit-vxc">
    <comment>Voxel collection Sandbox VoxEdit</comment>
    <glob pattern="*.vxc"/>
    <magic priority="50">
      <match type="string" offset="0" value="VXC"/>
    </magic>
  </mime-type>
  <mime-type type="application/x-build-engine">
    <comment>Voxel model used by the Build engine</comment>
    <glob pattern="*.kvx"/>
  </mime-type>
  <mime-type type="application/x-voxel3d">
    <comment>Voxel model used by Voxel3D</comment>
    <glob pattern="*.v3a"/>
  </mime-type>
  <mime-type type="application/x-slab6">
    <comment>Voxel model used by the SLAB6 editor</comment>
    <magic priority="50">
      <match type="string" offset="0" value="Kvxl"/>
    </magic>
    <glob pattern="*.kv6"/>
  </mime-type>
  <mime-type type="application/x-cc-vxl">
    <comment>Voxel model used by Command and Conquer</comment>
    <magic priority="50">
      <match type="string" offset="0" value="Voxel Animation"/>
    </magic>
    <glob pattern="*.vxl"/>
  </mime-type>
  <mime-type type="application/x-aos-vxl">
    <comment>Voxel model used by Ace Of Spades</comment>
    <glob pattern="*.vxl"/>
  </mime-type>
  <mime-type type="application/x-chronvox-studio-csm">
    <comment>Chronovox Studio Model</comment>
    <magic priority="50">
      <match type="string" offset="0" value=".CSM"/>
    </magic>
    <glob pattern="*.csm"/>
  </mime-type>
  <mime-type type="application/x-chronvox-studio-nvm">
    <comment>Nicks Voxel Model</comment>
    <magic priority="50">
      <match type="string" offset="0" value=".NVM"/>
    </magic>
    <glob pattern="*.nvm"/>
  </mime-type>
  <mime-type type="application/x-vengi">
    <comment>Vengi Voxel Scene</comment>
    <magic priority="50">
      <match type="string" offset="0" value="VENG"/>
    </magic>
    <glob pattern="*.vengi"/>
  </mime-type>
  <mime-type type="application/x-quake-md2">
    <comment>Quake2 Model</comment>
    <magic priority="50">
      <match type="string" offset="0" value="IDP2"/>
    </magic>
    <glob pattern="*.md2"/>
  </mime-type>
  <mime-type type="application/x-quake2-bsp">
    <comment>Quake2 Map</comment>
    <magic priority="50">
      <match type="string" offset="0" value="IBSP"/>
    </magic>
    <glob pattern="*.bsp"/>
  </mime-type>
  <mime-type type="application/x-voxelbuilder">
    <comment>VoxelBuilder</comment>
    <magic priority="50">
      <match type="string" offset="0" value="; Vo"/>
    </magic>
    <glob pattern="*.vbx"/>
  </mime-type>
</mime-info>
